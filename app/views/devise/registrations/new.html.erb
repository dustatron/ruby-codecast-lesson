
  <div class="row">
    <div class="col-md-4 col-md-offset-4 text-center">
      <% if params[:plan] == '2' %>
        <h1 class="text-center">Pro Account</h1>
        <p>Sign up For a Pro Account for $10 dollars a month and gain access to the community and contact info!</p>
        
      <% else%>
       <h1 class="text-center">Free Account</h1>
       <p>Sign up for a free account to gain access to the community.</p>

      <% end %>
      
    </div>

    
    <div class="col-md-6 col-md-offset-3">
      <div class = 'well'>
        <% if params[:plan] == '2' %>
          <%= render 'pro_form' %>
        <% else %>
          <%= render 'basic_form' %>
        <% end %>
    <div class="text-center">
         <%= render "devise/shared/links" %>
    </div>
 
    </div>
      
    </div>
    
  </div>

